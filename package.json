{
  "name": "@ircam/jsdoc-template",
  "version": "1.1.1",
  "description": "A clean, responsive documentation template theme for JSDoc 3 - forked from docdash",
  "main": "publish.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jsdoc -c fixtures/fixtures.conf.json",
    "sync": "browser-sync start -s ../fixtures-doc -f ../fixtures-doc --reload-delay 1000 --no-ui --no-notify",
    "watch": "watch-run -d 1000 -p tmpl/**,static/** \"npm run test\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clenemt/docdash.git"
  },
  "devDependencies": {
    "browser-sync": "latest",
    "jsdoc": "latest",
    "watch-run": "latest"
  },
  "author": "Clement Moron <clement.moron@gmail.com>",
  "license": "Apache-2.0",
  "keywords": [
    "jsdoc",
    "template"
  ],
  "files": [
    "publish.js",
    "static",
    "tmpl"
  ],
  "dependencies": {
    "@jsdoc/salty": "^0.2.1"
  }
}
